{
    "projectName": "serverless-todo-app",
    "version": "3.0",
    "frontend": "javascript",
    "javascript": {
        "framework": "none",
        "config": {
            "SourceDir": "src",
            "DistributionDir": "dist/pwa",
            "BuildCommand": "quasar build -m pwa",
            "StartCommand": "quasar serve --history dist/pwa"
        }
    },
    "providers": [
        "awscloudformation"
    ]
}